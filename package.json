{
  "name": "honeycup",
  "version": "1.0.0",
  "description": "Making Casual Dating Safe",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "testall": "mocha --experimental-modules --exit 'tests/**/*.test.js'",
    "test": "mocha --experimental-modules 'tests/services/azureBlob.service.test.js'",
    "debug": "node --inspect app.js",
    "start": "node app.js",
    "jest-test": "jest jest_test/media.controller.jest_test.js",
    "jest-testall": "jest --verbose",
    "testall-combined": "npm run jest-testall && npm run testall"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": [
      "**/jest_test/**/*.jest_test.js"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "^12.17.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "chai-http": "^5.0.0",
    "esm": "^3.2.25",
    "express": "^4.19.2",
    "firebase": "^10.11.0",
    "firebase-admin": "^12.1.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.0",
    "multer": "^1.4.3",
    "passport": "^0.7.0",
    "passport-apple": "^2.0.2",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "proxyquire": "^2.1.3",
    "public-ip": "^6.0.2",
    "redis": "^4.6.13",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "babel-jest": "^29.7.0",
    "chai": "^4.4.1",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "mocha": "^10.4.0",
    "mongodb-memory-server": "^9.1.8",
    "passport-mock-strategy": "^2.0.0",
    "proxyquireify": "^3.2.1",
    "sinon": "^17.0.1",
    "supertest": "^6.3.4"
  }
}
